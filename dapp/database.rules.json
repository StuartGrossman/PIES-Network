{
  "rules": {
    ".read": false,
    ".write": false,
    "user": {
      ".read": false,
      ".write": false,
      "$user_id": {
        // grants write access to the owner of this user account
        // whose uid must exactly match the key ($user_id)
        ".write": "$user_id === auth.uid",
        ".read": "$user_id === auth.uid"

      }
    },
    "usertype": {
      ".read": false,
    	".write": false,
      "$user_id": {
        // grants write access to the owner of this user account
        // whose uid must exactly match the key ($user_id)
        ".write": "$user_id === auth.uid",
        ".read": "$user_id === auth.uid"

      }
    },
    "terms": {
      ".read": false,
      ".write": false
    },
    "tag": {
      ".read": false,
    	".write": false,
      "$user_id": {

        // grants write access to the owner of this user account
        // whose uid must exactly match the key ($user_id)
        // ".write": "$user_id === auth.uid",
        ".read": "$user_id === auth.uid"

      }
    },
